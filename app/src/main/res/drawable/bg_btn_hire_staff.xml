<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_focused="true">
        <shape android:shape="rectangle">
            <gradient android:angle="90" android:endColor="#298dcd" android:startColor="#298dcd" android:type="linear" />
            <corners android:radius="5dp" />

        </shape>
    </item>
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <gradient android:angle="90" android:endColor="#298dcd" android:startColor="#298dcd" android:type="linear" />
            <corners android:radius="5dp" />

        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <gradient android:angle="90" android:endColor="#3daaee" android:startColor="#298dcd" android:type="linear" />
            <corners android:radius="5dp" />

        </shape>
    </item>
</selector>